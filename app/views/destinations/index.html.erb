<h1>All destinations<h1>
<% @destinations.each do |destination| %>
<%= destination.name %><br>
<%= cl_image_tag destination.photo %>
<% end %>

<%= link_to "New", new_destination_path(@destination) %>

<!-- MAP -->
<div
  id="map"
  style="width: 100%;
  height: 600px;"
  data-markers="<%= @markers.to_json %>"
></div>


